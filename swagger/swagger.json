{
  "swagger": "2.0",
  "info": {
    "description": "API for import objects",
    "version": "1.0.0",
    "title": "Waivio import service",
    "termsOfService": "https://hive.blog/@waivio"
  },
  "host": "localhost:8085",
  "tags": [
    {
      "name": "import",
      "description": "import objects"
    }
  ],
  "schemes": [
    "https",
    "http"
  ],
  "paths": {
    "/import-objects-service/import-products": {
      "post": {
        "tags": [
          "import"
        ],
        "summary": "starts new import",
        "produces": [
          "application/json"
        ],
        "consumes": ["multipart/form-data"],
        "parameters": [
          {
            "name": "access-token",
            "in": "header",
            "type": "string"
          },
          {
            "in": "formData",
            "type": "file",
            "name": "file",
            "description": "**file** -  file in form data",
            "required": true
          },
          {
            "in": "formData",
            "type": "string",
            "name": "user",
            "required": true
          },
          {
            "in": "formData",
            "type": "string",
            "name": "objectType",
            "required": true,
            "enum": ["book"]
          },
          {
            "in": "formData",
            "type": "string",
            "name": "authority",
            "enum": ["administrative", "ownership"]
          },
          {
            "in": "formData",
            "type": "string",
            "name": "locale"
          },
          {
            "in": "formData",
            "type": "boolean",
            "name": "translate"
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "$ref": "#/definitions/import-products-out-post"
            }
          },
          "422": {
            "description": "Validation error"
          }
        }
      },
      "get": {
        "tags": [
          "import"
        ],
        "summary": "get active import",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "user",
            "in": "query",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/importStatistic"
              }
            }
          },
          "422": {
            "description": "Validation error"
          }
        }
      },
      "put": {
        "tags": [
          "import"
        ],
        "summary": "starts new import",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "access-token",
            "in": "header",
            "type": "string"
          },
          {
            "in": "body",
            "name": "params",
            "description": "update import details",
            "required": true,
            "schema": {
              "$ref": "#/definitions/import-products-in-put"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
                "$ref": "#/definitions/importStatistic"
            }
          },
          "422": {
            "description": "Validation error"
          }
        }
      },
      "delete": {
        "tags": [
          "import"
        ],
        "summary": "starts new import",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "access-token",
            "in": "header",
            "type": "string"
          },
          {
            "in": "body",
            "name": "params",
            "description": "delete import",
            "required": true,
            "schema": {
              "$ref": "#/definitions/import-products-in-delete"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "$ref": "#/definitions/importStatistic"
            }
          },
          "422": {
            "description": "Validation error"
          }
        }
      }
    },
    "/import-objects-service/import-products/history": {
      "get": {
        "tags": [
          "import"
        ],
        "summary": "get active import",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "user",
            "in": "query",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/importStatistic"
              }
            }
          },
          "422": {
            "description": "Validation error"
          }
        }
      }
    },
    "/import-objects-service/import-products/power": {
      "get": {
        "tags": [
          "import"
        ],
        "summary": "get import power",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "user",
            "in": "query",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/import-products-power"
              }
            }
          },
          "422": {
            "description": "Validation error"
          }
        }
      },
      "put": {
        "tags": [
          "import"
        ],
        "summary": "update minVotingPower",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "access-token",
            "in": "header",
            "type": "string"
          },
          {
            "in": "body",
            "name": "params",
            "description": "update import minVotingPower",
            "required": true,
            "schema": {
              "$ref": "#/definitions/import-products-power"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "$ref": "#/definitions/import-products-power"
            }
          },
          "422": {
            "description": "Validation error"
          }
        }
      }
    },
    "/import-objects-service/amazon-asins": {
      "post": {
        "tags": [
          "import"
        ],
        "summary": "get asins query by uri",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "params",
            "description": "get asins query by uri",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "uri": {
                  "type": "string"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "type": "object",
              "properties": {
                "result": {
                  "type": "string"
                }
              }
            }
          },
          "422": {
            "description": "Validation error"
          }
        }
      }
    }
  },
  "definitions": {
    "import-products-power": {
      "type": "object",
      "properties": {
        "user": {
          "type": "string"
        },
        "minVotingPower": {
          "type": "number",
          "minimum": 1,
          "maximum": 10000
        }
      }
    },
    "import-products-in-delete": {
      "type": "object",
      "properties": {
        "user": {
          "type": "string"
        },
        "importId": {
          "type": "string"
        }
      }
    },
    "import-products-in-put": {
      "type": "object",
      "properties": {
        "user": {
          "type": "string"
        },
        "status": {
          "type": "string",
          "enum": ["active", "onHold"]
        },
        "importId": {
          "type": "string"
        }
      }
    },
    "import-products-in-post": {
      "type": "object",
      "properties": {
        "file": {
          "type": "string"
        },
        "user": {
          "type": "string"
        },
        "objectType": {
          "type": "string",
          "enum": [
            "book"
          ]
        },
        "authority": {
          "type": "string",
          "enum": [
            "administrative",
            "ownership"
          ]
        }
      }
    },
    "import-products-out-post": {
      "type": "object",
      "properties": {
        "result": {
          "type": "string"
        }
      }
    },
    "importStatistic": {
      "type": "object",
      "properties": {
        "user": {
          "type": "string"
        },
        "importId": {
          "type": "string"
        },
        "objectsCount": {
          "type": "number"
        },
        "objectsLastCount": {
          "type": "number"
        },
        "objectType": {
          "type": "string"
        },
        "authority": {
          "type": "string"
        },
        "createdAt": {
          "type": "string"
        },
        "updatedAt": {
          "type": "string"
        }
      }
    }
  }
}
