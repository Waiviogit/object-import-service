{
  "swagger": "2.0",
  "info": {
    "description": "API for import objects",
    "version": "1.0.0",
    "title": "Waivio import service",
    "termsOfService": "https://hive.blog/@waivio"
  },
  "host": "localhost:8085",
  "tags": [
    {
      "name": "import",
      "description": "import objects"
    }
  ],
  "schemes": [
    "https",
    "http"
  ],
  "paths": {
    "/import-objects-service/import-products": {
      "post": {
        "tags": [
          "import"
        ],
        "summary": "starts new import",
        "produces": [
          "application/json"
        ],
        "consumes": ["multipart/form-data"],
        "parameters": [
          {
            "name": "access-token",
            "in": "header",
            "type": "string"
          },
          {
            "in": "formData",
            "type": "file",
            "name": "file",
            "description": "**file** -  file in form data",
            "required": true
          },
          {
            "in": "formData",
            "type": "string",
            "name": "user",
            "required": true
          },
          {
            "in": "formData",
            "type": "string",
            "name": "objectType",
            "required": true,
            "enum": ["book"]
          },
          {
            "in": "formData",
            "type": "string",
            "name": "authority",
            "enum": ["administrative", "ownership"]
          },
          {
            "in": "formData",
            "type": "string",
            "name": "importName"
          },
          {
            "in": "formData",
            "type": "number",
            "name": "minVotingPower",
            "minimum": 1,
            "maximum": 10000,
            "default": 7000
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "$ref": "#/definitions/import-products-out-post"
            }
          },
          "422": {
            "description": "Validation error"
          }
        }
      },
      "get": {
        "tags": [
          "import"
        ],
        "summary": "get active import",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "user",
            "in": "query",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/importStatistic"
              }
            }
          },
          "422": {
            "description": "Validation error"
          }
        }
      },
      "put": {
        "tags": [
          "import"
        ],
        "summary": "starts new import",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "access-token",
            "in": "header",
            "type": "string"
          },
          {
            "in": "body",
            "name": "params",
            "description": "update import details",
            "required": true,
            "schema": {
              "$ref": "#/definitions/import-products-in-put"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
                "$ref": "#/definitions/importStatistic"
            }
          },
          "422": {
            "description": "Validation error"
          }
        }
      },
      "delete": {
        "tags": [
          "import"
        ],
        "summary": "starts new import",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "access-token",
            "in": "header",
            "type": "string"
          },
          {
            "in": "body",
            "name": "params",
            "description": "delete import",
            "required": true,
            "schema": {
              "$ref": "#/definitions/import-products-in-delete"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "$ref": "#/definitions/importStatistic"
            }
          },
          "422": {
            "description": "Validation error"
          }
        }
      }
    }
  },
  "definitions": {
    "import-products-in-delete": {
      "type": "object",
      "properties": {
        "user": {
          "type": "string"
        },
        "importId": {
          "type": "string"
        }
      }
    },
    "import-products-in-put": {
      "type": "object",
      "properties": {
        "user": {
          "type": "string"
        },
        "status": {
          "type": "string",
          "enum": ["active", "onHold"]
        },
        "name": {
          "type": "string"
        },
        "importId": {
          "type": "string"
        },
        "minVotingPower": {
          "type": "string",
          "minimum": 1,
          "maximum": 10000
        }
      }
    },
    "import-products-in-post": {
      "type": "object",
      "properties": {
        "file": {
          "type": "string"
        },
        "user": {
          "type": "string"
        },
        "objectType": {
          "type": "string",
          "enum": [
            "book"
          ]
        },
        "authority": {
          "type": "string",
          "enum": [
            "administrative",
            "ownership"
          ]
        },
        "importName": {
          "type": "string"
        },
        "minVotingPower": {
          "type": "number",
          "maximum": 10000,
          "minimum": 1
        }
      }
    },
    "import-products-out-post": {
      "type": "object",
      "properties": {
        "result": {
          "type": "string"
        }
      }
    },
    "importStatistic": {
      "type": "object",
      "properties": {
        "user": {
          "type": "string"
        },
        "importId": {
          "type": "string"
        },
        "objectsCount": {
          "type": "number"
        },
        "objectsLastCount": {
          "type": "number"
        },
        "name": {
          "type": "string"
        },
        "minVotingPower": {
          "type": "number"
        },
        "createdAt": {
          "type": "string"
        },
        "updatedAt": {
          "type": "string"
        }
      }
    }
  }
}
